{
  "name": "ness",
  "description": "âœª No-effort static sites deployed to your AWS account.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nessjs/ness.git"
  },
  "bin": {
    "ness": "bin/ness"
  },
  "scripts": {
    "no-changes": "(git log --oneline -1 | grep -q \"chore(release):\") && echo \"No changes to release.\"",
    "bump": "yarn --silent no-changes || standard-version",
    "release": "yarn --silent no-changes || (yarn bump && git push --follow-tags origin main)",
    "format": "prettier --write '**/*.*' && eslint . --ext .ts --fix",
    "package": "rm -fr dist && mkdir -p dist/js && yarn pack && mv *.tgz dist/js/",
    "compile": "tsc",
    "watch": "tsc -w",
    "build": "yarn test && yarn compile && yarn run package",
    "test": "rm -fr lib/ && jest --passWithNoTests --updateSnapshot && yarn eslint",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "eslint": "eslint . --ext .ts"
  },
  "author": {
    "name": "Adam Elmore",
    "email": "elmore.adam@gmail.com",
    "organization": false
  },
  "devDependencies": {
    "@types/git-branch": "2.0.1",
    "@types/ink": "^2.0.3",
    "@types/ink-spinner": "^3.0.0",
    "@types/ink-text-input": "^2.0.1",
    "@types/inquirer": "7.3.1",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.11",
    "@types/react": "^17.0.0",
    "@types/update-notifier": "5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "json-schema": "^0.2.5",
    "prettier": "2.2.1",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {},
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "1.77.0",
    "@aws-cdk/aws-cloudfront": "1.77.0",
    "@aws-cdk/aws-iam": "1.77.0",
    "@aws-cdk/aws-lambda": "1.77.0",
    "@aws-cdk/aws-route53": "1.77.0",
    "@aws-cdk/aws-route53-patterns": "1.77.0",
    "@aws-cdk/aws-route53-targets": "1.77.0",
    "@aws-cdk/aws-s3": "1.77.0",
    "@aws-cdk/aws-s3-deployment": "1.77.0",
    "@aws-cdk/core": "1.77.0",
    "@aws-cdk/custom-resources": "1.77.0",
    "@cloudcomponents/cdk-deletable-bucket": "^1.12.0",
    "aws-cdk": "1.77.0",
    "aws-sdk": "2.806.0",
    "chalk": "4.1.0",
    "commander": "6.2.0",
    "execa": "5.0.0",
    "final-form": "^4.20.1",
    "git-branch": "2.0.1",
    "ink": "^3.0.8",
    "ink-link": "^2.0.0",
    "ink-spinner": "^4.0.1",
    "ink-text-input": "^4.0.1",
    "inquirer": "7.3.3",
    "is-ci": "^2.0.0",
    "ora": "5.1.0",
    "react": "^17.0.1",
    "react-final-form": "^6.5.2",
    "terminal-link": "2.1.1",
    "update-notifier": "5.0.1"
  },
  "bundledDependencies": [],
  "keywords": [],
  "engines": {
    "node": ">= 10.0.0"
  },
  "license": "MIT",
  "version": "0.1.6",
  "types": "lib/index.d.ts",
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "preset": "ts-jest",
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  }
}
